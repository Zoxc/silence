struct Map(K Hash, V, A Allocator = Mem)
	struct Entry
		key K
		value V

	tbl PtrArray(Option(Entry), A)
	mask uint
	entries uint

	action create(allocator, size) :
			mask <- size - 1,
			tbl <- PtrArray(allocator)
			tbl.alloc(size)

			times(size, |i| Constructor[Option.None].construct(&tbl(i), ()))
	
	action copy

	action destroy
		tbl.destroy_n(mask + 1)